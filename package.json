{
  "name": "Buttercup",
  "version": "1.8.1",
  "private": true,
  "scripts": {
    "build:android": "react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "format": "prettier --write '{{source,scripts,__tests__}/**/*.js,index.*.js,shim.js}'",
    "nodeify": "rn-nodeify --install --hack",
    "postinstall": "npm run nodeify",
    "precommit": "lint-staged",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:android": "react-native run-android",
    "start:ios": "react-native run-ios --simulator='iPhone XS'",
    "start:ios:6s": "react-native run-ios --simulator='iPhone 6s'",
    "test": "run-s test:format test:react-native",
    "test:android": "./scripts/testAndroid.sh",
    "test:format": "prettier-check '{{source,scripts,__tests__}/**/*.js,index.*.js,shim.js}'",
    "test:react-native": "NODE_ENV=test jest --no-cache",
    "i18next:parse-lang": "i18next-scanner --config ./config/i18next-scanner.config.js 'source/**/*.{js,jsx}'"
  },
  "lint-staged": {
    "{{source,scripts,__tests__}/**/*.js,index.*.js,shim.js}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@buttercup/dropbox-client": "^0.1.1",
    "@perrymitchell/react-native-prompt": "^1.1.0",
    "@react-native-community/async-storage": "^1.3.4",
    "@tradle/react-native-http": "^2.0.0",
    "@yfuks/react-native-action-sheet": "0.0.5",
    "assert": "^1.4.1",
    "browserify-zlib": "^0.2.0",
    "buffer": "^3.6.0",
    "buttercup": "^2.8.1",
    "console-browserify": "^1.1.0",
    "constants-browserify": "0.0.1",
    "debounce": "^1.2.0",
    "dns.js": "^1.0.1",
    "domain-browser": "^1.1.7",
    "dropbox": "^4.0.13",
    "events": "^1.1.1",
    "extract-domain": "^2.0.4",
    "https-browserify": "0.0.1",
    "i18next": "^15.1.0",
    "is-online": "^7.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "0.0.0",
    "process": "^0.11.10",
    "prop-types": "^15.6.1",
    "querystring-es3": "^0.2.1",
    "react": "^16.8.6",
    "react-i18next": "^9.0.10",
    "react-native": "^0.59.5",
    "react-native-browser": "^0.6.2",
